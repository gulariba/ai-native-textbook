{
  "framework": "docusaurus",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/chat",
      "destination": "/api/chat/route.js"
    },
    {
      "source": "/api/chat/:path*",
      "destination": "/api/chat/route.js"
    }
  ]
}