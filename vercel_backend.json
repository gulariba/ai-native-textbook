{
  "version": 2,
  "name": "ai-native-book-backend",
  "builds": [
    {
      "src": "api/chatbot/start_server.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/chatbot/start_server.py"
    }
  ]
}